{
    "extends": [
      "next",
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"]
    },
    "overrides": [
        {
            "files": ["public/*.js"],
            "rules": {
                "@typescript-eslint/no-unused-expressions": "off"
            }
        }
    ]

  }
  